{"ast":null,"code":"import { Chart as ChartJS, CategoryScale, LinearScale, PointElement, LineElement, BarElement, Title, Tooltip, Legend } from 'chart.js';\nimport dashboard from './Sidebar.vue';\nimport { Line, Bar } from 'vue-chartjs';\nimport planetChartData from '../common/chart_dash.js';\nimport { API_USER } from '../common/contants';\nimport { count } from 'console';\nChartJS.register(CategoryScale, LinearScale, PointElement, LineElement, BarElement, Title, Tooltip, Legend);\nexport default {\n  name: 'dashboard.chartjs',\n  components: {\n    dashboard,\n    Line,\n    Bar\n  },\n  data() {\n    return {\n      planetChartData: planetChartData,\n      user: []\n    };\n  },\n  methods: {\n    getAllUserCount() {\n      this.$request.get(API_USER).then(result => {\n        this.user = result.data;\n        this.user.forEach(element => {\n          if (element.role === 'user') {\n            console.log(count(element));\n          }\n        });\n      }).catch(err => {\n        alert(err);\n      });\n    }\n  },\n  created() {\n    this.getAllUserCount();\n  }\n};","map":{"version":3,"names":["Chart","ChartJS","CategoryScale","LinearScale","PointElement","LineElement","BarElement","Title","Tooltip","Legend","dashboard","Line","Bar","planetChartData","API_USER","count","register","name","components","data","user","methods","getAllUserCount","$request","get","then","result","forEach","element","role","console","log","catch","err","alert","created"],"sources":["/home/longdh@kaopiz.local/Music/vueMiniProject/miniproject/src/views/admin/authors/Dashboard.vue"],"sourcesContent":["<template>\n    <div class=\" d-flex\">\n        <dashboard></dashboard>\n\n        <div class=\"container chart-info d-flex\">\n            <div class=\"count-user\" :key=\"u.id\"  v-for=\"u in user\" v-show=\"u.role === 'user'\"> \n                {{ }}\n            </div>\n            <div class=\"new-chart\">\n                <Line :data=\"planetChartData.chartData\" :options=\"planetChartData.options\" />\n            </div>\n            \n        </div>\n\n\n    </div>\n</template>\n<script>\nimport {\n    Chart as ChartJS,\n    CategoryScale,\n    LinearScale,\n    PointElement,\n    LineElement,\n    BarElement,\n    Title,\n    Tooltip,\n    Legend\n} from 'chart.js'\nimport dashboard from './Sidebar.vue'\nimport { Line, Bar } from 'vue-chartjs'\nimport planetChartData from '../common/chart_dash.js'\nimport { API_USER } from '../common/contants'\nimport { count } from 'console'\nChartJS.register(\n    CategoryScale,\n    LinearScale,\n    PointElement,\n    LineElement,\n    BarElement,\n    Title,\n    Tooltip,\n    Legend\n)\n\nexport default {\n    name: 'dashboard.chartjs',\n    components: {\n        dashboard,\n        Line,\n        Bar\n    },\n    data() {\n        return {\n            planetChartData: planetChartData,\n            user: []\n        }\n    },\n    methods: {\n        getAllUserCount() {\n            this.$request.get(API_USER).then((result) => {\n                this.user = result.data\n                this.user.forEach(element => {\n                    if (element.role ==='user') {\n                        console.log(count(element))\n                    }\n                });\n            }).catch((err) => {\n                alert(err)\n            });\n        }\n    },\n    created() {\n        this.getAllUserCount()\n    }\n    \n}\n</script>"],"mappings":"AAkBA,SACIA,KAAI,IAAKC,OAAO,EAChBC,aAAa,EACbC,WAAW,EACXC,YAAY,EACZC,WAAW,EACXC,UAAU,EACVC,KAAK,EACLC,OAAO,EACPC,MAAK,QACF,UAAS;AAChB,OAAOC,SAAQ,MAAO,eAAc;AACpC,SAASC,IAAI,EAAEC,GAAE,QAAS,aAAY;AACtC,OAAOC,eAAc,MAAO,yBAAwB;AACpD,SAASC,QAAO,QAAS,oBAAmB;AAC5C,SAASC,KAAI,QAAS,SAAQ;AAC9Bd,OAAO,CAACe,QAAQ,CACZd,aAAa,EACbC,WAAW,EACXC,YAAY,EACZC,WAAW,EACXC,UAAU,EACVC,KAAK,EACLC,OAAO,EACPC,MACJ;AAEA,eAAe;EACXQ,IAAI,EAAE,mBAAmB;EACzBC,UAAU,EAAE;IACRR,SAAS;IACTC,IAAI;IACJC;EACJ,CAAC;EACDO,IAAIA,CAAA,EAAG;IACH,OAAO;MACHN,eAAe,EAAEA,eAAe;MAChCO,IAAI,EAAE;IACV;EACJ,CAAC;EA