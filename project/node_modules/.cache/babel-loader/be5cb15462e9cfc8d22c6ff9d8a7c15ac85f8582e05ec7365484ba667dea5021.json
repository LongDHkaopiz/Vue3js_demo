{"ast":null,"code":"import { Chart as ChartJS, CategoryScale, LinearScale, PointElement, LineElement, BarElement, ArcElement, Title, Tooltip, Legend } from 'chart.js';\nimport dashboard from './Sidebar.vue';\nimport { Line, Bar, Doughnut } from 'vue-chartjs';\nimport { planetChartData, chartUser } from '../common/chart_dash.js';\nimport chartProduct from '../common/chartProduct.js';\nChartJS.register(CategoryScale, LinearScale, PointElement, LineElement, BarElement, ArcElement, Title, Tooltip, Legend);\nexport default {\n  name: 'dashboard.chartjs',\n  components: {\n    dashboard,\n    Line,\n    Bar,\n    Doughnut\n  },\n  data() {\n    return {\n      planetChartData: planetChartData,\n      chartUser: chartUser,\n      chartProduct: chartProduct,\n      date: new Date()\n    };\n  }\n};","map":{"version":3,"names":["Chart","ChartJS","CategoryScale","LinearScale","PointElement","LineElement","BarElement","ArcElement","Title","Tooltip","Legend","dashboard","Line","Bar","Doughnut","planetChartData","chartUser","chartProduct","register","name","components","data","date","Date"],"sources":["/home/longdh@kaopiz.local/Music/vueMiniProject/miniproject/src/views/admin/authors/Dashboard.vue"],"sourcesContent":["<template>\n    <div class=\" d-flex\">\n        <dashboard></dashboard>\n        <div class=\"container chart-info \">\n            <div class=\"content-chart d-flex pb-5\">\n                <div class=\"new-chart user\">\n                    <Bar :data=\"chartUser.chartData\" :options=\"chartUser.options\" style=\"height: 300px; width: 500px;\" />\n                </div>\n                <div class=\"new-chart product\">\n                    <Doughnut :data=\"chartProduct.chartData\" :options=\"chartProduct.options\" style=\"height: 300px; width: 400px;\" />\n                </div>\n            </div>\n            <div class=\"new-chart total-actual\">\n                <Line :data=\"planetChartData.chartData\" :options=\"planetChartData.options\" style=\"width:600px;\" />\n            </div>\n            <el-calendar v-model=\"date\" style=\"width: 700px;\"/>\n        </div>\n\n\n    </div>\n</template>\n<script>\nimport {\n    Chart as ChartJS,\n    CategoryScale,\n    LinearScale,\n    PointElement,\n    LineElement,\n    BarElement,\n    ArcElement,\n    Title,\n    Tooltip,\n    Legend\n} from 'chart.js'\nimport dashboard from './Sidebar.vue'\nimport { Line, Bar, Doughnut } from 'vue-chartjs'\nimport { planetChartData, chartUser } from '../common/chart_dash.js'\nimport chartProduct from '../common/chartProduct.js'\nChartJS.register(\n    CategoryScale,\n    LinearScale,\n    PointElement,\n    LineElement,\n    BarElement,\n    ArcElement,\n    Title,\n    Tooltip,\n    Legend\n)\n\nexport default {\n    name: 'dashboard.chartjs',\n    components: {\n        dashboard,\n        Line,\n        Bar,\n        Doughnut\n\n    },\n    data() {\n        return {\n            planetChartData: planetChartData,\n            chartUser: chartUser,\n            chartProduct: chartProduct,\n            date: new Date()\n        }\n    },\n\n}\n</script>"],"mappings":"AAsBA,SACIA,KAAI,IAAKC,OAAO,EAChBC,aAAa,EACbC,WAAW,EACXC,YAAY,EACZC,WAAW,EACXC,UAAU,EACVC,UAAU,EACVC,KAAK,EACLC,OAAO,EACPC,MAAK,QACF,UAAS;AAChB,OAAOC,SAAQ,MAAO,eAAc;AACpC,SAASC,IAAI,EAAEC,GAAG,EAAEC,QAAO,QAAS,aAAY;AAChD,SAASC,eAAe,EAAEC,SAAQ,QAAS,yBAAwB;AACnE,OAAOC,YAAW,MAAO,2BAA0B;AACnDhB,OAAO,CAACiB,QAAQ,CACZhB,aAAa,EACbC,WAAW,EACXC,YAAY,EACZC,WAAW,EACXC,UAAU,EACVC,UAAU,EACVC,KAAK,EACLC,OAAO,EACPC,MACJ;AAEA,eAAe;EACXS,IAAI,EAAE,mBAAmB;EACzBC,UAAU,EAAE;IACRT,SAAS;IACTC,IAAI;IACJC,GAAG;IACHC;EAEJ,CAAC;EACDO,IAAIA,CAAA,EAAG;IACH,OAAO;MACHN,eAAe,EAAEA,eAAe;MAChCC,SAAS,EAAEA,SAAS;MACpBC,YAAY,EAAEA,YAAY;MAC1BK,IAAI,EAAE,IAAIC,IAAI,CAAC;IACnB;EACJ;AAEJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}