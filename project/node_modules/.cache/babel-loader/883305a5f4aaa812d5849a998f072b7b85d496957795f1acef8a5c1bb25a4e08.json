{"ast":null,"code":"import { Chart as ChartJS, CategoryScale, LinearScale, PointElement, LineElement, BarElement, Title, Tooltip, Legend } from 'chart.js';\nimport dashboard from './Sidebar.vue';\nimport { Line, Bar } from 'vue-chartjs';\nimport planetChartData from '../common/chart_dash.js';\nimport { API_USER } from '../common/contants';\nChartJS.register(CategoryScale, LinearScale, PointElement, LineElement, BarElement, Title, Tooltip, Legend);\nexport default {\n  name: 'dashboard.chartjs',\n  components: {\n    dashboard,\n    Line,\n    Bar\n  },\n  data() {\n    return {\n      planetChartData: planetChartData,\n      user: []\n    };\n  },\n  getAllUser(userId) {\n    this.$request.get(API_USER + `/${userId}`).then(result => {\n      this.user = result.data;\n      console.log(this.user.length);\n    }).catch(err => {\n      alert(err);\n    });\n  }\n};","map":{"version":3,"names":["Chart","ChartJS","CategoryScale","LinearScale","PointElement","LineElement","BarElement","Title","Tooltip","Legend","dashboard","Line","Bar","planetChartData","API_USER","register","name","components","data","user","getAllUser","userId","$request","get","then","result","console","log","length","catch","err","alert"],"sources":["/home/longdh@kaopiz.local/Music/vueMiniProject/miniproject/src/views/admin/authors/Dashboard.vue"],"sourcesContent":["<template>\n    <div class=\" d-flex\">\n        <dashboard></dashboard>\n\n        <div class=\"container chart-info\">\n            <div class=\"new-chart\">\n                <Line :data=\"planetChartData.chartData\" :options=\"planetChartData.options\" />\n            </div>\n        </div>\n\n\n    </div>\n</template>\n<script>\nimport {\n    Chart as ChartJS,\n    CategoryScale,\n    LinearScale,\n    PointElement,\n    LineElement,\n    BarElement,\n    Title,\n    Tooltip,\n    Legend\n} from 'chart.js'\nimport dashboard from './Sidebar.vue'\nimport { Line, Bar } from 'vue-chartjs'\nimport planetChartData from '../common/chart_dash.js'\nimport { API_USER } from '../common/contants'\nChartJS.register(\n    CategoryScale,\n    LinearScale,\n    PointElement,\n    LineElement,\n    BarElement,\n    Title,\n    Tooltip,\n    Legend\n)\n\nexport default {\n    name: 'dashboard.chartjs',\n    components: {\n        dashboard,\n        Line,\n        Bar\n    },\n    data() {\n        return {\n            planetChartData: planetChartData,\n            user: []\n        }\n    },\n    getAllUser(userId) {\n        this.$request.get(API_USER + `/${userId}`).then((result) => {\n            this.user = result.data\n            console.log(this.user.length)\n        }).catch((err) => {\n            alert(err)\n        });\n    }\n}\n</script>"],"mappings":"AAcA,SACIA,KAAI,IAAKC,OAAO,EAChBC,aAAa,EACbC,WAAW,EACXC,YAAY,EACZC,WAAW,EACXC,UAAU,EACVC,KAAK,EACLC,OAAO,EACPC,MAAK,QACF,UAAS;AAChB,OAAOC,SAAQ,MAAO,eAAc;AACpC,SAASC,IAAI,EAAEC,GAAE,QAAS,aAAY;AACtC,OAAOC,eAAc,MAAO,yBAAwB;AACpD,SAASC,QAAO,QAAS,oBAAmB;AAC5Cb,OAAO,CAACc,QAAQ,CACZb,aAAa,EACbC,WAAW,EACXC,YAAY,EACZC,WAAW,EACXC,UAAU,EACVC,KAAK,EACLC,OAAO,EACPC,MACJ;AAEA,eAAe;EACXO,IAAI,EAAE,mBAAmB;EACzBC,UAAU,EAAE;IACRP,SAAS;IACTC,IAAI;IACJC;EACJ,CAAC;EACDM,IAAIA,CAAA,EAAG;IACH,OAAO;MACHL,eAAe,EAAEA,eAAe;MAChCM,IAAI,EAAE;IACV;EACJ,CAAC;EACDC,UAAUA,CAACC,MAAM,EAAE;IACf,IAAI,CAACC,QAAQ,CAACC,GAAG,CAACT,QAAO,GAAK,IAAGO,MAAO,EAAC,CAAC,CAACG,IAAI,CAAEC,MAAM,IAAK;MACxD,IAAI,CAACN,IAAG,GAAIM,MAAM,CAACP,IAAG;MACtBQ,OAAO,CAACC,GAAG,CAAC,IAAI,CAACR,IAAI,CAACS,MAAM;IAChC,CAAC,CAAC,CAACC,KAAK,CAAEC,GAAG,IAAK;MACdC,KAAK,CAACD,GAAG;IACb,CAAC,CAAC;EACN;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}