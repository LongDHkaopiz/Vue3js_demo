if (res.data.success) {\n                        this.$router.push({\n                            name: 'user.list'\n                        })\n                        return\n                    }\n                    alert('Something went wrong')\n                }\n            )\n        },\n        getUser(Userid) {\n            this.$request.get(API_USER + `/${Userid}`).then((result) => {\n                this.user = result.data\n            }).catch((err) => {\n                alert(err)\n            });\n        },\n        cancel() {\n            this.user = {\n                name: \"\",\n                email: \"\",\n                password: \"\",\n            }\n        }\n    }\n\n}\n</script>"],"mappings":";AAqDA,SAASA,QAAO,QAAS,oBAAmB;AAC5C,eAAe;EACXC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,IAAI,EAAE;QACFF,IAAI,EAAE,EAAE;QACRG,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE;MACd,CAAC;MACDC,MAAM,EAAE;QACJL,IAAI,EAAE,EAAE;QACRG,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE;MACd;IACJ;EACJ,CAAC;EACDE,OAAO,EAAE;IACLC,QAAQA,CAAA,EAAG;MACP,IAAIC,OAAM,GAAI,IAAG;MACjB,IAAI,CAACH,MAAK,GAAI;QACVL,IAAI,EAAE,EAAE;QACRS,KAAK,EAAE,EAAE;QACTC,WAAW,EAAE,EAAE;QACfC,KAAK,EAAE;MACX;MACA,IAAI,CAAC,IAAI,CAACT,IAAI,CAACF,IAAI,EAAE;QACjB,IAAI,CAACK,MAAM,CAACL,IAAG,GAAI,uBAAsB;QACzCQ,OAAM,GAAI,KAAI;MAClB,CAAC,MAAK,IAAI,CAAC,IAAI,CAACN,IAAI,CAACC,KAAK,EAAE;QACxB,IAAI,CAACE,MAAM,CAACF,KAAI,GAAI,wBAAuB;QAC3CK,OAAM,GAAI,KAAI;MAClB,CAAC,MAAK,IAAI,CAAC,IAAI,CAACN,IAAI,CAACE,QAAQ,EAAE;QAC3B,IAAI,CAACC,MAAM,CAACD,QAAO,GAAI,2BAA0B;QACjDI,OAAM,GAAI,KAAI;MAClB;MACA,OAAOA,OAAM;IACjB,CAAC;IACDI,IAAIA,CAAA,EAAG;MACH,IAAI,IAAI,CAACL,QAAQ,CAAC,CAAC,EAAE,CAErB;MACA,IAAI,CAACM,QAAQ,CAACC,IAAI,CAACf,QAAQ,EAAE,IAAI,CAACG,IAAI,CAAC,CAACa,IAAI,CACxCC,GAAE,IAAK;QACH,IAAIA,GAAG,CAACf,IAAI,CAACgB,OAAO,EAAE;UAClB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;YACdnB,IAAI,EAAE;UACV,CAAC;UACD;QACJ;QACAoB,KAAK,CAAC,sBAAsB;MAChC,CACJ;IACJ,CAAC;IACDC,OAAOA,CAACC,MAAM,EAAE;MACZ,IAAI,CAACT,QAAQ,CAACU,GAAG,CAACxB,QAAO,GAAK,IAAGuB,MAAO,EAAC,