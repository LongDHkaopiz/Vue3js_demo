{"ast":null,"code":"import { API_NEWS, API_NEWS_KEY } from '../common/contants';\nimport dashboard from './Sidebar.vue';\nexport default {\n  name: 'admin.news',\n  components: {\n    dashboard\n  },\n  data() {\n    return {\n      news: [],\n      search: '',\n      date_from: '',\n      date_to: ''\n    };\n  },\n  methods: {\n    async getAllNews() {\n      var new_date_from = this.convertDateTime(this.date_from);\n      var new_date_to = this.convertDateTime(this.date_to);\n      const url = API_NEWS + `?q=${this.search}&from=${new_date_from}&to=${new_date_to}&sortBy=popularity&` + API_NEWS_KEY;\n      try {\n        const response = await fetch(url);\n        const rest = await response.json();\n        this.news = rest;\n        console.log(this.date_from);\n      } catch (error) {\n        console.error(error);\n      }\n    },\n    convertDateTime(value) {\n      let dateObj = new Date(value);\n      let year = dateObj.getFullYear();\n      let month = (\"0\" + (dateObj.getMonth() + 1)).slice(-2);\n      let day = (\"0\" + dateObj.getDate()).slice(-2);\n      let formattedDate = `${year}-${month}-${day}`;\n      return formattedDate;\n    }\n  },\n  created() {}\n};","map":{"version":3,"names":["API_NEWS","API_NEWS_KEY","dashboard","name","components","data","news","search","date_from","date_to","methods","getAllNews","new_date_from","convertDateTime","new_date_to","url","response","fetch","rest","json","console","log","error","value","dateObj","Date","year","getFullYear","month","getMonth","slice","day","getDate","formattedDate","created"],"sources":["/home/longdh@kaopiz.local/Music/vueMiniProject/miniproject/src/views/admin/authors/News.vue"],"sourcesContent":["<template>\n    <div class=\"d-flex\">\n        <dashboard></dashboard>\n        <div class=\"container\">\n            <el-form :model=\"form\" label-width=\"120px\">\n                <el-form-item label=\"Search By\">\n                    <el-input v-model=\"search\" />\n                </el-form