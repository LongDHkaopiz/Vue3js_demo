{"ast":null,"code":"import dashboard from './Sidebar.vue';\nexport default {\n  name: 'dashboard.anime',\n  components: {\n    dashboard\n  },\n  data() {\n    return {\n      animes: {}\n    };\n  },\n  methods: {\n    async getAllAnime() {\n      const url = 'https://animes5.p.rapidapi.com/';\n      const options = {\n        method: 'GET',\n        headers: {\n          'X-RapidAPI-Key': 'dde66f0561msh324e6dc8a212669p1f94d2jsna8556e10d67a',\n          'X-RapidAPI-Host': 'animes5.p.rapidapi.com'\n        }\n      };\n      try {\n        const response = await fetch(url, options);\n        const rest = await response.json();\n        this.animes = rest.animes;\n      } catch (error) {\n        console.error(error);\n      }\n    }\n  },\n  created() {\n    this.getAllAnime();\n  }\n};\nconst imageContainer = document.querySelector('.image-container');\nconst image = imageContainer.querySelector('img');\nconst textOverlay = imageContainer.querySelector('.text-overlay');\nimageContainer.addEventListener('mouseenter', () => {\n  image.style.transform = 'scale(1.2)';\n  textOverlay.style.opacity = 1;\n});\nimageContainer.addEventListener('mouseleave', () => {\n  image.style.transform = 'scale(1)';\n  textOverlay.style.opacity = 0;\n});","map":{"version":3,"names":["dashboard","name","components","data","animes","methods","getAllAnime","url","options","method","headers","response","fetch","rest","json","error","console","created","imageContainer","document","querySelector","image","textOverlay","addEventListener","style","transform","opacity"],"sources":["/home/longdh@kaopiz.local/Music/vueMiniProject/miniproject/src/views/admin/authors/Anime.vue"],"sourcesContent":["<template>\n    <div class=\"d-flex\">\n        <dashboard></dashboard>\n        <div class=\"container list-anime \" style=\" display: flex;\n                    flex-wrap: wrap;\n                    align-items: center;\n                justify-content: space-between;\n        \">\n            <div class=\"anime-item d-flex\" :key=\"index\" v-for=\"(anime, index) in animes\" style=\"width: calc(100% / 3);\">\n                <img :src=\"anime.main_picture.medium\" :alt=\"anime.title\">\n            </div>\n\n        </div>\n        <div class=\"image-container\">\n            <img src=\"your-image.jpg\" alt=\"Your Image\">\n            <div class=\"text-overlay\">\n                <p>Duwosi</p>\n            </div>\n        </div>\n    </div>\n</template>\n<script>\nimport dashboard from './Sidebar.vue'\nexport default {\n    name: 'dashboard.anime',\n    components: {\n        dashboard,\n    },\n    data() {\n        return {\n            animes: {}\n        }\n    },\n    methods: {\n        async getAllAnime() {\n            const url = 'https://animes5.p.rapidapi.com/';\n            const options = {\n                method: 'GET',\n                headers: {\n                    'X-RapidAPI-Key': 'dde66f0561msh324e6dc8a212669p1f94d2jsna8556e10d67a',\n                    'X-RapidAPI-Host': 'animes5.p.rapidapi.com'\n                }\n            };\n            try {\n                const response = await fetch(url, options);\n                const rest = await response.json();\n                this.animes = rest.animes\n            } catch (error) {\n                console.error(error);\n            }\n        }\n    },\n    created() {\n        this.getAllAnime()\n    },\n}\nconst imageContainer = document.querySelector('.image-container');\nconst image = imageContainer.querySelector('img');\nconst textOverlay = imageContainer.querySelector('.text-overlay');\n\nimageContainer.addEventListener('mouseenter', () => {\n    image.style.transform = 'scale(1.2)';\n    textOverlay.style.opacity = 1;\n});\n\nimageContainer.addEventListener('mouseleave', () => {\n    image.style.transform = 'scale(1)';\n    textOverlay.style.opacity = 0;\n});\n</script>\n<style scoped>\n.image-container {\n  position: relative;\n  width: 300px;\n  height: 200px;\n  overflow: hidden;\n}\n\n.text-overlay {\n  position: absolute;\n  bottom: 0;\n  left: 0;\n  right: 0;\n  background: rgba(0, 0, 0, 0.5);\n  color: #fff;\n  padding: 10px;\n  text-align: center;\n  opacity: 0;\n}\n</style>"],"mappings":"AAsBA,OAAOA,SAAQ,MAAO,eAAc;AACpC,eAAe;EACXC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,MAAM,EAAE,CAAC;IACb;EACJ,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,WAAWA,CAAA,EAAG;MAChB,MAAMC,GAAE,GAAI,iCAAiC;MAC7C,MAAMC,OAAM,GAAI;QACZC,MAAM,EAAE,KAAK;QACbC,OAAO,EAAE;UACL,gBAAgB,EAAE,oDAAoD;UACtE,iBAAiB,EAAE;QACvB;MACJ,CAAC;MACD,IAAI;QACA,MAAMC,QAAO,GAAI,MAAMC,KAAK,CAACL,GAAG,EAAEC,OAAO,CAAC;QAC1C,MAAMK,IAAG,GAAI,MAAMF,QAAQ,CAACG,IAAI,CAAC,CAAC;QAClC,IAAI,CAACV,MAAK,GAAIS,IAAI,CAACT,MAAK;MAC5B,EAAE,OAAOW,KAAK,EAAE;QACZC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACxB;IACJ;EACJ,CAAC;EACDE,OAAOA,CAAA,EAAG;IACN,IAAI,CAACX,WAAW,CAAC;EACrB;AACJ;AACA,MAAMY,cAAa,GAAIC,QAAQ,CAACC,aAAa,CAAC,kBAAkB,CAAC;AACjE,MAAMC,KAAI,GAAIH,cAAc,CAACE,aAAa,CAAC,KAAK,CAAC;AACjD,MAAME,WAAU,GAAIJ,cAAc,CAACE,aAAa,CAAC,eAAe,CAAC;AAEjEF,cAAc,CAACK,gBAAgB,CAAC,YAAY,EAAE,MAAM;EAChDF,KAAK,CAACG,KAAK,CAACC,SAAQ,GAAI,YAAY;EACpCH,WAAW,CAACE,KAAK,CAACE,OAAM,GAAI,CAAC;AACjC,CAAC,CAAC;AAEFR,cAAc,CAACK,gBAAgB,CAAC,YAAY,EAAE,MAAM;EAChDF,KAAK,CAACG,KAAK,CAACC,SAAQ,GAAI,UAAU;EAClCH,WAAW,CAACE,KAAK,CAACE,OAAM,GAAI,CAAC;AACjC,CAAC,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}