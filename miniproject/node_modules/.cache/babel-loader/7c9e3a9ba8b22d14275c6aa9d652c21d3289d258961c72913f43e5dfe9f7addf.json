yUrl = 'https://cors-anywhere.herokuapp.com/'\n            const agent = new HttpsProxyAgent(proxyUrl);\n            const url = API_NEWS + `?q=${this.search}&from=${this.new_date_from}&to=${this.new_date_to}&sortBy=popularity&apiKey=` + API_NEWS_KEY;\n\n            try {\n                const response = await fetch(url, { agent });\n                const rest = await response.json();\n                console.log(rest);\n                // this.news = rest\n                // console.log(this.news);\n            } catch (error) {\n                console.error(error);\n            }\n        },\n        convertDateTime(value) {\n            let dateObj = new Date(value);\n            let year = dateObj.getFullYear();\n            let month = (\"0\" + (dateObj.getMonth() + 1)).slice(-2);\n            let day = (\"0\" + dateObj.getDate()).slice(-2);\n            let formattedDate = `${year}-${month}-${day}`;\n            return formattedDate;\n        }\n    },\n    created() {\n\n    }\n}\n</script>"],"mappings":"AA6BA,SAASA,QAAQ,EAAEC,YAAW,QAAS,oBAAmB;AAC1D,OAAOC,SAAQ,MAAO,eAAc;AACpC,eAAe;EACXC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,IAAI,EAAE,EAAE;MACRC,MAAM,EAAE,EAAE;MACVC,SAAS,EAAE,EAAE;MACbC,OAAO,EAAE,EAAE;MACXC,aAAa,EAAE,EAAE;MACjBC,WAAW,EAAE;IACjB;EACJ,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,UAAUA,CAAA,EAAG;MACf,MAAMC,KAAI,GAAIC,OAAO,CAAC,YAAY,CAAC;MACnC,MAAMC,eAAc,GAAID,OAAO,CAAC,mBAAmB,CAAC;MACpD,IAAI,CAACL,aAAY,GAAI,IAAI,CAACO,eAAe,CAAC,IAAI,CAACT,SAAS;MACxD,IAAI,CAACG,WAAU,GAAI,IAAI,CAACM,eAAe,CAAC,IAAI,CAACR,OAAO;MACpD,MAAMS,QAAO,GAAI,sCAAqC;MACtD,MAAMC,KAAI,GAAI,IAAIH,eAAe,CAACE,QAAQ,CAAC;MAC3C,MAAME,GAAE,GAAIpB,QAAO,GAAK,MAAK,IAAI,CAACO,MAAO,SAAQ,IAAI,CAACG,aAAc,OAAM,IAAI,CAACC,WAAY,4BAA0B,GAAIV,YAAY;MAErI,IAAI;QACA,MAAMoB,QAAO,GAAI,MAAMP,KAAK,CAACM,GAAG,EAAE;UAAED;QAAM,CAAC,CAAC;QAC5C,MAAMG,IAAG,GAAI,MAAMD,QAAQ,CAACE,IAAI,CAAC,CAAC;QAClCC,OAAO,CAACC,GAAG,CAACH,IAAI,CAAC;QACjB;QACA;MACJ,EAAE,OAAOI,KAAK,EAAE;QACZF,O