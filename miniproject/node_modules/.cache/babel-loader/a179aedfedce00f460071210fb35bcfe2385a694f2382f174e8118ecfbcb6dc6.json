rl = 'https://animes5.p.rapidapi.com/';\n            const options = {\n                method: 'GET'\n\n            };\n            try {\n                const response = await fetch(url, options);\n                const rest = await response.json();\n                this.news = rest\n            } catch (error) {\n                console.error(error);\n            }\n        }\n    },\n}\n</script>"],"mappings":"AAuDA,SAASA,QAAO,QAAS,oBAAmB;AAC5C,OAAOC,SAAQ,MAAO,eAAc;AACpC,eAAe;EACXC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,IAAI,EAAE;IACV;EACJ,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,UAAUA,CAAA,EAAG;MACf,MAAMC,GAAE,GAAI,iCAAiC;MAC7C,MAAMC,OAAM,GAAI;QACZC,MAAM,EAAE;MAEZ,CAAC;MACD,IAAI;QACA,MAAMC,QAAO,GAAI,MAAMC,KAAK,CAACJ,GAAG,EAAEC,OAAO,CAAC;QAC1C,MAAMI,IAAG,GAAI,MAAMF,QAAQ,CAACG,IAAI,CAAC,CAAC;QAClC,IAAI,CAACT,IAAG,GAAIQ,IAAG;MACnB,EAAE,OAAOE,KAAK,EAAE;QACZC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACxB;IACJ;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}