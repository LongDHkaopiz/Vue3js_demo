this.search}&from=${this.new_date_from}&to=${this.new_date_to}&sortBy=popularity&apiKey=` + API_NEWS_KEY;\n\n            try {\n                const response = await fetch(url);\n                const rest = await response.json();\n                console.log(rest);\n                // this.news = rest\n                // console.log(this.news);\n            } catch (error) {\n                console.error(error);\n\n            }\n        },\n        convertDateTime(value) {\n            let dateObj = new Date(value);\n            let year = dateObj.getFullYear();\n            let month = (\"0\" + (dateObj.getMonth() + 1)).slice(-2);\n            let day = (\"0\" + dateObj.getDate()).slice(-2);\n            let formattedDate = `${year}-${month}-${day}`;\n            return formattedDate;\n        }\n    },\n    created() {\n\n    },\n    setup() {\n        const news = ref([]);\n\n        onMounted(() => {\n            // Gọi hàm fetchNews để lấy tin tức\n            fetchNews();\n        });\n\n        return {\n            news,\n        };\n    },\n}\n</script>"],"mappings":"AA6BA,SAASA,QAAQ,EAAEC,YAAW,QAAS,oBAAmB;AAC1D,OAAOC,SAAQ,MAAO,eAAc;AACpC,SAASC,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,OAAO,SAAS,EAAE;AAClB,eAAe;EACXC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACRJ;EACJ,CAAC;EACDK,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,IAAI,EAAE,EAAE;MACRC,MAAM,EAAE,EAAE;MACVC,SAAS,EAAE,EAAE;MACbC,OAAO,EAAE,EAAE;MACXC,aAAa,EAAE,EAAE;MACjBC,WAAW,EAAE;IACjB;EACJ,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,UAAUA,CAAA,EAAG;MACf,IAAI,CAACH,aAAY,GAAI,IAAI,CAACI,eAAe,CAAC,IAAI,CAACN,SAAS;MACxD,IAAI,CAACG,WAAU,GAAI,IAAI,CAACG,eAAe,CAAC,IAAI,CAACL,OAAO;MACpD,MAAMM,GAAE,GAAIjB,QAAO,GAAK,oBAAmB,IAAI,CAACS,MAAO,SAAQ,IAAI,CAACG,aAAc,OAAM,IAAI,CAACC,WAAY,4BAA0B,GAAIZ,YAAY;MAEnJ,IAAI;QACA,MAAMiB,QAAO,GAAI,MAAMC,KAAK,CAACF,GAAG,CAAC;QACjC,MAAMG,IAAG,GAAI,MAAMF,QAAQ,CAACG,IAAI,CAAC,CAAC;QAClCC,OAAO,CAACC,GAAG,CAACH,IAAI,CAAC;QACjB;QACA;MACJ,EAAE,OAAOI,KAAK,EAAE;QACZF,OAAO,CAACE,KAAK,CAACA,KAAK,CAAC;MAExB;IACJ,CAAC;IACDR,eAAeA